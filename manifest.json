{
  "name": "Gitspeed",
  "version": "1.0",
  "description": "Gitspeed will do something with git......",
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["http://localhost:5001/*"],
      "js": ["oauth.js"]
    },
    { "matches": ["http://*/*", "https://*/*"], "js": ["content.js"] }
  ],
  "browser_action": {
    "default_title": "Blah blah blah"
  },
  "background": {
    "scripts": ["axios.min.js", "backgroundOauth.js"],
    "persistent": false
  },
  "manifest_version": 2
}
